{
  "name": "wdio-cucumber",
  "type": "module",
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/fs-extra": "^11.0.4",
    "@wdio/allure-reporter": "^9.12.6",
    "@wdio/cli": "^9.12.7",
    "@wdio/cucumber-framework": "^9.12.6",
    "@wdio/local-runner": "^9.12.7",
    "@wdio/spec-reporter": "^9.12.6",
    "chai": "^5.2.0",
    "ts-node": "^10.9.2",
    "wdio-wait-for": "^3.1.0"
  },
  "scripts": {
    "pretest": "node --loader ts-node/esm features/utils/pre-test.ts",
    "test": "wdio run ./wdio.conf.ts  --cucumberOpts.tags='@CC' > logs/spec-output.log 2>&1",
    "pretest2": "node --loader ts-node/esm features/utils/pre-test.ts",
    "test2": "wdio run ./wdio.conf.ts  --cucumberOpts.tags='@FirstTimeOrder_Scenario1'",
    "presmoke": "node --loader ts-node/esm features/utils/pre-test.ts",
    "smoke": "wdio run wdio.conf.ts --cucumberOpts.tagExpression='@Login_Scenario1'",
    "report": "allure generate allure-results --clean"
  },
  "dependencies": {
    "allure-commandline": "^2.34.0",
    "dotenv": "^16.5.0",
    "fs-extra": "^11.3.0",
    "moment-timezone": "^0.5.48",
    "winston": "^3.17.0"
  }
}